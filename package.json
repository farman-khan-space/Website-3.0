{
  "name": "website-3.0",
  "version": "1.0.0",
  "description": "Portfolio website for Farman Khan, Data Analyst.",
  "main": ".eleventy.js",
  "scripts": {
    "watch:css": "npx tailwindcss -i ./css/style.css -o ./_site/css/style.css --watch",
    "build:css": "npx tailwindcss -i ./css/style.css -o ./_site/css/style.css --minify",
    "start": "npx concurrently \"npm run watch:css\" \"npx @11ty/eleventy --serve\"",
    "build": "npm run build:css && npx @11ty/eleventy"
  },
  "author": "Farman Khan",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.3"
  }
}
